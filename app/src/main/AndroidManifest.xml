<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ugiant.myandroidview">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.BottomWayActivity"></activity>
        <activity android:name=".activity.BottomTabTabLayoutActivity"></activity>

        <activity android:name=".activity.PracticeActivity"></activity>
        <activity android:name=".activity.GetStartActivity"></activity>
        <activity android:name=".activity.activity_get_start.TextViewActivity"></activity>
        <activity android:name=".activity.activity_get_start.ViewActivity"></activity>
        <activity android:name=".activity.activity_get_start.TopBarActivity"></activity>
        <activity android:name=".activity.activity_get_start.AudioBarChatViewActivity"></activity>

    </application>

</manifest>